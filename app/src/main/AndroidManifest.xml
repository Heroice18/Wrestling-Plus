<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wrestlingtournament">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".manageTournaments"
            android:screenOrientation="portrait"/>
        <activity android:name=".teamManage"
            android:screenOrientation="portrait"/>
        <activity android:name=".CreateTournamentActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".MatchActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".TournamentActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Login_Start"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>